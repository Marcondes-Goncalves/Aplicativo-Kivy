
<HomePage>:
    FloatLayout:

        canvas:
            Rectangle:
                size:self.size
                pos: self.pos
                source: "icones/bg1.png"

        LabelButton:
            pos_hint: {"right": 1, "top": 1}
            size_hint: 1, 0.85
            text: "Estou na HomePage - ir para pagina ajustes"
            on_release: app.mudarTela("ajustespage")


        Button:
            pos_hint: {"right": 1, "top": 0.15}
            size_hint: 0.33, 0.15
            text: "Ir para ajustes"
            on_release: app.mudarTela("ajustespage")


